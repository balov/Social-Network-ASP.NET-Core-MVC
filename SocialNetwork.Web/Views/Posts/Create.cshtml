@model PostFormModel
@{
    ViewData["Title"] = "Create Post";
}
<h2 class="text-center">@ViewData["Title"]</h2>
<div class="row">
    <div class="col-md-4 col-md-offset-4">
        @Html.Partial("_PostForm", Model)
    </div>
</div>

@section Scripts {
    @await Html.PartialAsync("_ValidationScriptsPartial")
}